        {
          "arg": "limit",
          "type": "number",
          "required": false
        },
        {
          "arg": "offset",
          "type": "number",
          "required": false
        }
      ],
      "returns": {
        "arg": "result",
        "type": "array",
        "root": true
      },
      "description": "Retrieve instances of a model by model name",
      "http": {
        "path": "/getModelItems",
        "verb": "get"
      }
    }
  }
