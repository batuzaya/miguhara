          "required": true
        },
        {
          "arg": "modelId",
          "type": "string",
          "required": true
        }
      ],
      "returns": {
        "arg": "result",
        "type": "object",
        "root": true
      },
      "description": "Retrieve an instance of a model by model name and id",
      "http": {
        "path": "/getModelItem",
        "verb": "get"
