    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {
    "getModels": {
      "isStatic": true,
      "accepts": [],
      "returns": {
        "arg": "result",
        "type": "array",
        "root": true
      },
